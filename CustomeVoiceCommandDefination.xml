<?xml version="1.0" encoding="utf-8" ?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CustomCommands">
   <CommandPrefix> Listen Up </CommandPrefix>
    <Example> Open File, Open Website, Add a rectangle </Example>
    
    <Command Name="OpenWebsite">
      <Example> Check my Business License </Example>          
      <ListenFor> Check my Business License </ListenFor>    
      <Feedback> Confirming 8 your Business Updates </Feedback>
      <Navigate/>          
    </Command>

    <Command Name="OpenFile">
      <Example> Open file </Example>
      <ListenFor> Open file </ListenFor>
      <Feedback> Opening your file </Feedback>
      <Navigate/>
    </Command>

    

    <Command Name="CreateFile">
      <Example> Create file </Example>
      <ListenFor> Create file </ListenFor>
      <Feedback> Creating your file </Feedback>
      <Navigate/>
    </Command>

    
    <Command Name="SendSerialData">
      <Example> Send serial data </Example>
      <ListenFor> Send serial data </ListenFor>
      <Feedback> Sending serial data </Feedback>
      <Navigate/>
    </Command>
    
  <Command Name="addRectangle">
    <Example>add a rectangle</Example>
    <ListenFor>add [a] rectangle</ListenFor>
  <ListenFor>add a {color} rectangle</ListenFor>
    <Feedback>Adding a rectangle</Feedback>
    <Navigate/>
  </Command>
    
  <Command Name="addCircle">
    <Example>add a circle</Example>
    <ListenFor>add [a] circle</ListenFor>
    <ListenFor>add a {color} circle</ListenFor>
    <Feedback>Adding a circle</Feedback>
  <Navigate/>
  </Command>
    
  <PhraseList Label="color">
    <Item>Red</Item>
    <Item>Blue</Item>
  <Item>Green</Item>
  </PhraseList>

  </CommandSet>  

</VoiceCommands>
